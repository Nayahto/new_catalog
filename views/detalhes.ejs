<%- include('./components/head')%>
<body>
    <%- include('./components/header')%>
    <main>
        <div class="container">
            <div class="item">
                <img src="<%= filme.imagem%>" alt="capa do filme">
            </div>
            <div class="item_text">
                <h2><%= filme.nome%></h2>
                <p><%= filme.descricao%></p>
                <a href="/editar/<%= filme.id %>">editar</a>
                <a href="/deletar/<%= filme.id %>">excluir</a>
            </div>
        </div>
    </main>
    <%- include('./components/footer')%>
    <script src="./js/script.js"></script>
</body>
</html>