<%- include('./components/head')%>
<body>
    <%- include('./components/header')%>
    <main>
        <div class="container">
            <% filme.forEach(item => { %>
            <div class="item">
                <h2><%= item.nome %></h2>
                <p><%= item.descricao %></p>
                <a href="/filmes/:<%= filme.id %>"><img src="<%= item.imagem %>" alt="capa do filme"></a>
            </div>
            <% }) %>
        </div>
    </main>
    <%- include('./components/footer')%>
    <script src="./js/script.js"></script>
</body>

</html>